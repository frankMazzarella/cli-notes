{
  "extends": "tslint-config-airbnb",
  "rules": {
    "max-line-length": [true, 120],
    "no-boolean-literal-compare": true,
    "no-console": true,
    "no-debugger": true,
    "no-increment-decrement": false,
    "align": false,
    "variable-name": { "options": ["allow-leading-underscore"] }
  }
}
